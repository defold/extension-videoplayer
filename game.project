[project]
title = VideoPlayer
version = 0.1
custom_resources = videos

[bootstrap]
main_collection = /example/player.collectionc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 800
height = 600
variable_dt = 1

[physics]
scale = 0.02

[script]
shared_state = 1

[ios]
bundle_identifier = com.defoldexample.videoplayer

[android]
package = com.defoldexample.videoplayer

[osx]
bundle_identifier = com.defoldexample.videoplayer

[library]
include_dirs = videoplayer

